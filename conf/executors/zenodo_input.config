/*
 * -------------------------------------------------
 *  Nextflow input params on ZENODO storage
 * -------------------------------------------------
 * Defines bundled specific input data from google cloud platform.
 * Usage: -config conf/zenodo_params.config
 */

params{
	// Input used by pipeline, located in zenodo,
	// such as https://zenodo.org/record/5513090
	zenodoNumber="5513090"
	nanome_input_dir="https://zenodo.org/record/5513090/files"

	deepmod_ctar = "https://zenodo.org/record/5513090/files/C.tar.gz"
	genome_annotation = "https://zenodo.org/record/5513090/files/genome-annotation.tar.gz"
	genome = "https://zenodo.org/record/5513090/files/hg38.tar.gz"
	deepsignal_model_tar = "https://zenodo.org/record/5513090/files/model.CpG.R9.4_1D.human_hx1.bn17.sn360.v0.1.7%2B.tar.gz"
	megalodon_model_tar = "https://zenodo.org/record/5513090/files/megalodon_model.tar.gz"
}
